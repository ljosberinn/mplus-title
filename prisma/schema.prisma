generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model History {
  timestamp   Int
  id          Int      @id @default(autoincrement())
  faction     Factions
  region      Regions
  customRank  Int
  customScore Float
  rioRank     Int
  rioScore    Float
}

model CrossFactionHistory {
  id        Int     @id @default(autoincrement())
  timestamp Int
  region    Regions
  score     Float
  rank      Int
}

enum Factions {
  horde
  alliance
}

enum Regions {
  EU
  KR
  TW
  US
}

model DungeonHistory {
  id        Int    @id @default(autoincrement())
  timestamp Int
  slug      String
  keyLevel  Int
}
